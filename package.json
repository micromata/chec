{
  "name": "check-packages",
  "version": "1.1.3",
  "description": "CLI tool to check your npm dependencies against a list of allowed/forbidden packages.",
  "license": "MIT",
  "homepage": "https://github.com/micromata/check-packages",
  "repository": "micromata/check-packages",
  "author": {
    "name": "Christian KÃ¼hl",
    "email": "c.kuehl@micromata.de",
    "url": "micromata.de"
  },
  "bin": "cli.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watchAll",
    "coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "files": [
    "lib",
    "cli.js"
  ],
  "keywords": [
    "cli",
    "cli-app",
    "dependencies",
    "check-packages",
    "check",
    "packages",
    "dependency-tree",
    "dependency-checking",
    "whitelist",
    "blacklist"
  ],
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.js"
    ],
    "collectCoverageFrom": [
      "lib/**/*.js"
    ]
  },
  "dependencies": {
    "chalk": "2.4.2",
    "load-json-file": "6.2.0",
    "meow": "^7.0.1",
    "ora": "3.4.0",
    "semver": "6.3.0",
    "update-notifier": "3.0.1"
  },
  "devDependencies": {
    "coveralls": "3.0.6",
    "eslint": "6.3.0",
    "eslint-config-es": "3.8.0",
    "jest": "^26.0.1"
  }
}
