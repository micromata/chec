{
  "name": "check-packages",
  "version": "0.0.1",
  "description": "Checks the dependencies inside package.json against a list of allowed/forbidden packages.",
  "license": "MIT",
  "repository": "",
  "author": {
    "name": "Christian KÃ¼hl",
    "email": "c.kuehl@micromata.de",
    "url": "micromata.de"
  },
  "bin": "cli.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watchAll",
    "coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "files": [
    "lib",
    "cli.js"
  ],
  "keywords": [
    "cli",
    "cli-app",
    "dependency-tree",
    "dependency-checking",
    "whitelist",
    "blacklist"
  ],
  "dependencies": {
    "chalk": "2.4.1",
    "load-json-file": "5.1.0",
    "meow": "5.0.0",
    "ora": "3.0.0",
    "update-notifier": "2.5.0"
  },
  "devDependencies": {
    "coveralls": "3.0.2",
    "eslint": "5.6.0",
    "eslint-config-es": "0.9.1",
    "jest": "23.6.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/jest-setup.js",
    "collectCoverageFrom": [
      "lib/**/*.js"
    ]
  }
}
